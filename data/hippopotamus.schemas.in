<gconfschemafile>
  <schemalist>
    <schema>
      <key>/schemas/apps/hippopotamus/icecast/hostname</key>
      <applyto>/apps/hippopotamus/icecast/hostname</applyto>
      <owner>hippopotamus</owner>
      <type>string</type>
      <default>127.0.0.1</default>
      <locale name="C">
	<short>Hostname</short>
	<long>Hostname of the Icecast server</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/hippopotamus/icecast/mountpoint</key>
      <applyto>/apps/hippopotamus/icecast/mountpoint</applyto>
      <owner>hippopotamus</owner>
      <type>string</type>
      <default>/</default>
      <locale name="C">
	<short>Mountpoint</short>
	<long>Mountpoint to which the streaming will be done</long>
      </locale>
    </schema>    
    <schema>
      <key>/schemas/apps/hippopotamus/icecast/passwd</key>
      <applyto>/apps/hippopotamus/icecast/passwd</applyto>
      <owner>hippopotamus</owner>
      <type>string</type>
      <default>hackme</default>
      <locale name="C">
	<short>Password</short>
	<long>Source password of the Icecast server</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/hippopotamus/icecast/port</key>
      <applyto>/apps/hippopotamus/icecast/port</applyto>
      <owner>hippopotamus</owner>
      <type>int</type>
      <default>8000</default>
      <locale name="C">
	<short>Port</short>
	<long>Port on which the Icecast server is listening</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/hippopotamus/video/overlay</key>
      <applyto>/apps/hippopotamus/video/overlay</applyto>
      <owner>hippopotamus</owner>
      <type>string</type>
      <default>Hippo Power!!</default>
      <locale name="C">
	<short>Video Overlay</short>
	<long>Video Overlay to be displayed with the stream</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/hippopotamus/video/vquality</key>
      <applyto>/apps/hippopotamus/video/vquality</applyto>
      <owner>hippopotamus</owner>
      <type>int</type>
      <default>16</default>
      <locale name="C">
	<short>Video Quality</short>
	<long>Video quality setting for the encoder</long>
      </locale>
    </schema>
        
 </schemalist>
</gconfschemafile>
